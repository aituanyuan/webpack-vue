{
    "presets":[
        ["@babel/preset-env",{
            "modules": false
        }],
        ["@babel/preset-typescript",{
            "isTSX":true,
            "allExtensions":true
        }]
    ],
    "plugins":[
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-runtime",{
            "useESModules":true,
            "corejs":{
                "version": 3,
                "proposals": true
            }
        }],
        ["@vue/babel-plugin-jsx",{
            "optimize":true,
        }]
    ]
}